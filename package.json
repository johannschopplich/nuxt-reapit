{
  "name": "nuxt-reapit",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@8.9.0",
  "description": "Nuxt 3 module for Kirby's Query Language API",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/nuxt-reapit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/nuxt-reapit.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/nuxt-reapit/issues"
  },
  "keywords": [
    "getkirby",
    "kirby",
    "kql",
    "nuxt",
    "query"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "tsc --noEmit",
    "release": "bumpp --commit --push --tag",
    "prepare": "nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.4",
    "@reapit/connect-session": "^4.1.2",
    "ofetch": "^1.3.3",
    "unjwt": "^0.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.27",
    "@nuxt/module-builder": "^0.5.2",
    "@types/node": "^20.8.6",
    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "nuxt": "^3.7.4",
    "nuxt-reapit": "workspace:*",
    "typescript": "^5.2.2",
    "vue-tsc": "^1.8.19"
  }
}
